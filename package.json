{
  "name": "mern-eshop",
  "version": "1.0.0",
  "description": "MERN shopping app",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nafiur63/mern-e-shop.git"
  },
  "author": "Md Nafiur Reza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nafiur63/mern-e-shop/issues"
  },
  "homepage": "https://github.com/nafiur63/mern-e-shop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "mongoose": "^5.12.3"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
